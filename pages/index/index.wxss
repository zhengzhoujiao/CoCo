page{
  background: whitesmoke;
}
.nav swiper{
  height: 330rpx;
}
.nav swiper-item image{
  width: 100%;
  height: 200rpx;
}
.wx-swiper-dots{
  position: absolute;
  top: 270rpx;
}
.body{
  position: absolute;
  margin-top:-40rpx;
  width: 90%;
  margin-left: 5%;
}
.body .top{
  height: 648rpx;
  background-color: white;
  width: 100%;
  display: flex;
}
.body .top .topLeft{
  flex: 2;
}

.body .top .topRight{
  flex: 2;
  background: white;
  box-sizing: border-box;
  border: 2px orange solid;
  border-radius: 10rpx;
  text-align: center;
}
.body .top .topRight view:nth-child(1){
  font-weight: 600;
  color: orange;
  font-size: 24px;
  margin-top: 30rpx;
}
.body .top .topRight view:nth-child(2){
  color: orange;
  margin-top: 5rpx;
}
.body .top .topRight view:nth-child(3) image{
  margin-top: 20rpx;
  width: 80%;
  height: 380rpx;
}
.body .top .topRight view:nth-child(4){
  margin-top: 10rpx;
  font-size: 14px;
  color: orange;
}
.body .top .topRight view:nth-child(5){
  margin-top: 10rpx;
  font-size: 14px;
  color: gray;
}
.body .mid{
  position: relative;
  height: 80rpx;
  width: 100%;
  background-color: white;
  margin-top: 20rpx;
  box-sizing: border-box;
  border: 2px orange solid;
  border-radius: 10rpx;
  line-height: 70rpx;
}
.body .mid .midAction{
  display: inline-block;
  margin-left: 30rpx;
  font-weight: 600;
  color: orange;
}
.swiper-view{
  margin-top: -55rpx;
  margin-left: 110rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-radius: 5rpx;
 }
  
 .swiper_container {
  height: 50rpx;
  width: 90%;
 }
  
 .swiper_item {
  height: 50rpx;
  width: 90%;
  font-size: 26rpx;
  white-space: nowrap;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: gray
 }
.body .bottom{
  display: flex;
  justify-content: space-around;
}
.ceil{
  width: 315rpx;
  height: 315rpx;
  margin-top: 20rpx;
  background-color: white;
  border: 2px orange solid;
  border-radius: 10rpx;
  box-sizing: border-box;
}
.ceil view{
  margin-left: 50%;
  transform: translateX(-50%);
  text-align: center;
  width: 100%;
}
.ceil view:nth-child(2){
  width: 100%;
  color: orange;
  font-weight: 600;
  font-size: 24px;
  margin-top: 10rpx;
}
.ceil view:nth-child(3){
  color: gray;
  font-size: 12px;
  margin-top: 20rpx;
}
.ceil view image{
  width: 130rpx;
  height: 130rpx;
  margin-top: 30rpx;
}
.theCeil{
  box-sizing: border-box;
  border: 2px orange solid;
  border-radius: 10rpx;
  width: 315rpx;
  height: 315rpx;
  background-color: white;
}
.theCeil view{
  margin-left: 50%;
  transform: translateX(-50%);
  text-align: center;
}
.theCeil view:nth-child(2){
  width: 100%;
  color: orange;
  font-weight: 600;
  font-size: 24px;
  margin-top: 10rpx;
}
.theCeil view:nth-child(3){
  color: gray;
  font-size: 12px;
  margin-top: 20rpx;
}
.theCeil view image{
  width: 130rpx;
  height: 130rpx;
  margin-top: 30rpx;
}